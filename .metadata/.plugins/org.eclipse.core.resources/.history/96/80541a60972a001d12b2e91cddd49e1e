package sec3;

public class Print1 {

	public static void main(String[] args) {
		System.out.print("\na\n"); //출력 대용자(\n)= newline
		System.out.print("b\n");
		System.out.print("c\n");
		
		System.out.println("d\n"); //println= 해당 내용을 출력 후 줄바꿈을 진행한다. = \n
		
		System.out.println("번호\t이름\t국어\t영어\t수학\t총점");	//\t = 한글자 씩 띄어쓰기(tab정도 크기) 진행
		System.out.println("\\\"");	//\ 혹은 " 출력 시 사전에 \를 한 번 작성한다.
	}

}
