package sec2;

public class Board {
	private int num;
	private String sub;
	private String con;
	private String wri;
	private String date;

	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public String getSub() {
		return sub;
	}
	public void setSub(String sub) {
		this.sub = sub;
	}
	public String getCon() {
		return con;
	}
	public void setCon(String con) {
		this.con = con;
	}
	public String getWri() {
		return wri;
	}
	public void setWri(String wri) {
		this.wri = wri;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	
