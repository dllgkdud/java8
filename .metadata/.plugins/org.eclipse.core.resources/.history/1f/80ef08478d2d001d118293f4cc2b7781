package sec1;

public class Oper4 {

	public static void main(String[] args) {
		int a=200, b=100, c=20, d=10;
		//비교 연산자(==, !=, >, >=, <, <= true or false로 산출) - (법정용어) 파악★
		//== 같다 (이다)
		//!= 같지 않다(다르다)
		//> 크다(초과==후)
		//>= 크거나 같다(이상==이후)
		//< 작다(미만=전)
		//<= 작거나 같다(이하==이전)
		System.out.println("a==b => "+(a==b));	//false
		System.out.println("a!==b => "+(a!=b));	//true
		System.out.println("a>b => "+(a>b));	//true
		System.out.println("a>=b => "+(a>=b));	//true
		System.out.println("a<b => "+(a<b));	//false
		System.out.println("a<=b => "+(a<=b));	//false
	}

}
