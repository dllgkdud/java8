package sec1;
//제어문: 프로그램 상 작업의 흐름을 제어하기 위한 명령문	- 대부분의 명령문은 제어문에 해당한다.
public class Cont1 {

	public static void main(String[] args) {
		int a=90;
		if(a>=60) {
			pass();
		} else {
			fail();
		}	
	}
	public static void pass(){
		System.out.println("합격");
	}
	public static void fail(){
		System.out.println("불합격");
	}
}
